        "@typescript-eslint/parser": "^5.10.2",
        "eslint": "^8.57.0",
        "eslint-config-next": "14.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-simple-import-sort": "^7.0.0",
