        "@radix-ui/react-popover": "^1.0.6",
        "@radix-ui/react-separator": "1.0.2",
        "@radix-ui/react-tabs": "^1.0.4",
        "@radix-ui/react-visually-hidden": "^1.0.2",
        "@radix-ui/themes": "^2.0.0",
        "@vercel/analytics": "^1.3.1",
